                <script>
                    function getParameterByName(name, url) {
                        if (!url) {
                            url = window.location.href;
                        }
                        name = name.replace(/[\[\]]/g, "\\$&");
                        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                            results = regex.exec(url);
                        if (!results) return null;
                        if (!results[2]) return '';
                        return decodeURIComponent(results[2].replace(/\+/g, " "));
                    }
                    function putCat(lid, catm, catid) {

                        if (lid == null) {
                            lid = $.cookie("lid");
                        }

                        var isPost = getParameterByName("isPost");
                        save("catid", catid);
                        save("catm", catm);

                        if (isPost == null) {
                            window.location = "ads?lid=" + lid + "&catid=" + catid + "&catm=" + catm;
                        }
                        else if (isPost == "true") {


                            window.location = "post?cat=" + catid + "&lid" + lid + "&catm=" + catm;
                        }
                    }
                </script>
